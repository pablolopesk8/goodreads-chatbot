{
    "name": "goodreads-chatbot",
    "version": "0.0.1",
    "description": "Chatbot connected with Goodreads API",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pablolopesk8/goodreads-chatbot.git"
    },
    "keywords": [
        "chatbot",
        "goodreads",
        "api"
    ],
    "license": "MIT",
    "author": "Pablo Lopes",
    "main": "src/index.js",
